<div class="bookmark-form">
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <p class="btn btn-primary"><%= f.button :submit%></p>
  <% end %>
</div>

<%= link_to "Go Back to your List", list_path(@list.id), class:"btn btn-primary" %>
